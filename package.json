{
  "name": "@lastic/simple-git",
  "description": "Simple GIT interface for node.js, bundled with native git natived",
  "version": "1.124.0",
  "author": "Steve King <steve@mydev.co>",
  "contributors": [
    {
      "name": "Yulong Huang",
      "email": "yulong.huang@elastic.co"
    }
  ],
  "dependencies": {
    "debug": "^4.0.1"
  },
  "devDependencies": {
    "@kwsites/test-runner": "^0.1.1",
    "aws-sdk": "^2.503.0",
    "glob": "^7.1.4",
    "request": "^2.88.0",
    "sinon": "^7.3.2",
    "targz": "^1.0.1"
  },
  "keywords": [
    "git",
    "source control",
    "vcs"
  ],
  "license": "MIT",
  "repository": "https://github.com/elastic/simple-git.git",
  "main": "./src/index.js",
  "files": [
    "promise.*",
    "src/",
    "typings/",
    "scripts/"
  ],
  "scripts": {
    "test": "node test/runner.js test test/**/test*.js ",
    "test:unit": "node test/runner.js test/unit/test*.js",
    "test:integration": "node test/runner.js test/integration/test*.js",
    "preversion": "yarn test",
    "postversion": "npm publish && git push && git push --tags",
    "postinstall": "node scripts/postinstall.js"
  },
  "git_natives": {
    "linux": {
      "version": "2.22.0",
      "build": "1"
    },
    "darwin": {
      "version": "2.21.0",
      "build": "954b7fe"
    },
    "win32": {
      "version": "2.21.0",
      "build": "954b7fe"
    }
  }
}
